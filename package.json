{
  "name": "vue-blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "client": "nuxt",
    "server": "nodemon api/_server.js",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/proxy": "^2.0.1",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "core-js": "^3.6.5",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "http-proxy-middleware": "^1.0.5",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.24.1",
    "mavon-editor": "^2.9.0",
    "mongoose": "^5.9.27",
    "nuxt": "^2.14.0",
    "path": "^0.12.7",
    "vue-datetime": "^1.0.0-beta.14",
    "vue-fragment": "^1.5.1",
    "vue-meta": "^2.4.0",
    "vue-select": "^3.10.7",
    "vue-simple-markdown": "^1.1.4",
    "weekstart": "^1.0.1"
  },
  "devDependencies": {},
  "vercel": {
    "alias": "corey-lanier-blog",
    "name": "blog",
    "env": {
      "PORT": "4000",
      "SECRET": "@secret",
      "MONGODB_URI": "@mongodb_uri"
    },
    "version": 2,
    "builds": [
      {
        "src": "nuxt.config.js",
        "use": "@nuxtjs/vercel-builder",
        "config": {}
      }
    ],
    "routes": [
      {
        "src": "/api/posts/(.+)",
        "dest": "/api/posts.js"
      },
      {
        "src": "/api/users/(.+)",
        "dest": "/api/users.js"
      },
      {
        "src": "/api/comments/(.+)",
        "dest": "/api/comments.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
}
